<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Kompyuter</servlet-name>
        <servlet-class>KopyuterServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Laptop</servlet-name>
        <servlet-class>LaptopServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Printer</servlet-name>
        <servlet-class>PrinterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Kompyuter</servlet-name>
        <url-pattern>/pc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Laptop</servlet-name>
        <url-pattern>/laptop</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Printer</servlet-name>
        <url-pattern>/printer</url-pattern>
    </servlet-mapping>
    
</web-app>